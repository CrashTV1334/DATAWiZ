<%-include("../partials/header.ejs")%>
<div class="main-content">
    <div class="row">
        <div class="col-12">
            <div class="container dropdown-container" id="drpdwnCont">
                <select name="facility" id="facilitySel" class="mySelect">
                    <option value="" selected="selected">--Select--</option>
                </select>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <div class="container" id="dataContainer">
                <table>
                    <tr class="dataContent">
                        <td class="tag tag-name">Country </td>
                        <td class="tag">: </td>
                        <td class="num" id="country"></td>
                    </tr>
                    <tr class="dataContent">
                        <td class="tag tag-name">State </td>
                        <td class="tag">: </td>
                        <td class="num" id="state"></td>
                    </tr>
                    <tr class="dataContent">
                        <td class="tag tag-name">City </td>
                        <td class="tag">: </td>
                        <td class="num" id="city"></td>
                    </tr>

                    <tr class="dataContent">
                        <td class="tag tag-name">Condition </td>
                        <td class="tag">: </td>
                        <td class="num" id="condition"></td>
                    </tr>
                    <tr class="dataContent">
                        <td class="tag tag-name">Measure Name </td>
                        <td class="tag">: </td>
                        <td class="num" id="measure"></td>
                    </tr>
                    <tr class="dataContent">
                        <td class="tag tag-name">Samples </td>
                        <td class="tag">: </td>
                        <td class="num" id="samples"></td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <div class="container col-center-head" id="dataContainer">
                <!-- <table>
                    <tr class="dataContent">
                        <td>Score</td>
                    </tr>
                </table> -->
                <p class="dataContent">Score</p>
            </div>
            <div class="container">
                <div class="container index-col-center" id="scoreGraph">
                    <canvas id="analyse-pieChart"></canvas>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.min.js"></script>
<script src="/javascripts/veteranAPI_facility.js"></script>
