<%-include("../partials/header.ejs")%>
<div class="main-content">
    <div class="row">
        <div class="col-12">
            <div class="container col-center-head" id="dataContainer">
                <p class="dataContent" id="cc1">Score Outliers</p>
            </div>
            <div class="container">
                <div class="container col-center" id="scoreOutliersGraph">
                    <canvas id="bubbleChart"></canvas>
                </div>
            </div>
        </div>
    </div>
    <br>
    <table class="outliers-table cb">
        <tr class="outliers-table-head cb">
            <th colspan="2" class="cb">Score</th>
        </tr>
        <tr class="cb">
            <th class="cb">Outliers Type</th>
            <th class="cb">Outliers Count</th>
        </tr>
        <tr class="cb">
            <td class="cb">Low Outliers</td>
            <td class="cb" id="lo_sc">0</td>
        </tr>
        <tr class="cb">
            <td class="cb">High Outliers</td>
            <td class="cb" id="hi_sc">0</td>
        </tr>
    </table>
    <br>
    <br>
    <br>
    <div class="row">
        <div class="col-12">
            <div class="container col-center-head" id="dataContainer">
                <p class="dataContent" id="cc2">Sample Outliers</p>
            </div>
            <div class="container">
                <div class="container col-center" id="sampleOutliersGraph">
                    <canvas id="BbubbleChart"></canvas>
                </div>
            </div>
        </div>
    </div>
    <br>
    <table class="outliers-table cb">
        <tr class="outliers-table-head cb">
            <th colspan="2" class="cb">Samples</th>
        </tr>
        <tr class="cb">
            <th class="cb">Outliers Type</th>
            <th class="cb">Outliers Count</th>
        </tr>
        <tr class="cb">
            <td class="cb">Low Outliers</td>
            <td class="cb" id="lo_sm">0</td>
        </tr>
        <tr class="cb">
            <td class="cb">High Outliers</td>
            <td class="cb" id="hi_sm">0</td>
        </tr>
    </table>
</div>
<br>
<br>
<br>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.min.js"></script>
<script src="/javascripts/veteran/veteranAPI_outliers.js"></script>
<%-include("../partials/footer.ejs")%>